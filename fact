#define _CRT_SECURE_NO_WARNINGS 1
#include<stdio.h>
#include<stdio.h>
int main()
{
	int n; //循环控制变量 
	scanf("%d", &n);
	unsigned long long i, m = 1;//累乘的初始值为1 
	unsigned long long s = 0;//累加的初始值为0 
	for (i = 1; i <= 20; i++)
	{
		m = m * i;//累乘 
		s = s + m;//累加 
	}
	printf("%lld\n%e", s,s);//输出形式 
	return 0;
}
